void moveZeroes(int*nums, int numsSize)
    {
    int left=0;
    int right=0;
    while(right<numsSize)
        {
            if(nums[right]!=0)
                { nums[left]=nums[right];
                right++;
                left++;
                }
            else
                {
                right++;
                }   
        }
    for(int i=left;i<right;i++)
        {
            nums[i]=0;
        }
    return nums;
}
